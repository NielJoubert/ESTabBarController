GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = $(inherited) "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/ESTabBarController" "${PODS_ROOT}/Headers/Public/UIColor-HexString"
LIBRARY_SEARCH_PATHS = $(inherited) "$PODS_CONFIGURATION_BUILD_DIR/ESTabBarController" "$PODS_CONFIGURATION_BUILD_DIR/UIColor-HexString"
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/ESTabBarController" -isystem "${PODS_ROOT}/Headers/Public/UIColor-HexString"
OTHER_LDFLAGS = $(inherited) -ObjC -l"ESTabBarController" -l"UIColor-HexString"
PODS_BUILD_DIR = $BUILD_DIR
PODS_CONFIGURATION_BUILD_DIR = $PODS_BUILD_DIR/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)
PODS_ROOT = ${SRCROOT}/Pods
